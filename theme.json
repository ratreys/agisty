{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "color": {
      "background": false,
      "button": false,
      "caption": false,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "heading": false,
      "link": false,
      "text": false,
      "duotone": null,
      "gradients": null,
      "palette": null
    },
    "dimensions": {
      "defaultAspectRatios": false,
      "aspectRatios": null
    },
    "shadow": {
      "defaultPresets": false,
      "presets": null
    },
    "spacing": {
      "customSpacingSize": false,
      "defaultSpacingSizes": false,
      "units": ["rem", "vh", "vw", "%"],
      "spacingScale": null
    },
    "typography": {
      "customFontSize": false,
      "defaultFontSizes": false,
      "dropCap": false,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textAlign": false,
      "textDecoration": false,
      "textTransform": false,
      "writingMode": false,
      "fontSizes": null
    },
    "layout": {
      "contentSize": "750px",
      "wideSize": "1200px"
    },
    "custom": {
      "gray": "rgb(70 80 90)",
      "link": "rgb(10 20 30)",
      "brand": "rgb(100 50 100)",
      "black": "rgb(10 20 30)",
      "white": "rgb(250 250 250)",
      "focus": "rgb(250 190 70)",
      "button": "rgb(130 30 30)",
      "border": "rgb(from var(--wp--custom--gray) r g b / 29%)",
      "guides": "rgb(from var(--wp--custom--gray) r g b / 11%)"
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--custom--white)",
      "text": "var(--wp--custom--black)"
    },
    "elements": {
      "button": {
        "border": {
          "radius": ".5rem"
        },
        "color": {
          "background": "var(--wp--custom--button)",
          "text": "var(--wp--custom--white)"
        },
        "typography": {
          "fontSize": "clamp(0.9375rem, 0.8125rem + 0.4vw, 1rem)",
          "lineHeight": "1.2"
        }
      }
    },
    "blocks": {
      "core/pullquote": {
        "typography": {
          "fontSize": "inherit",
          "lineHeight": "1.5"
        }
      }
    }
  }
}
