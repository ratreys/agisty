{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"background": false,
			"button": false,
			"caption": false,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"heading": false,
			"link": false,
			"text": false,
			"duotone": null,
			"gradients": null,
			"palette": null
		},
		"dimensions": {
			"defaultAspectRatios": false,
			"aspectRatios": null
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content-size)",
			"wideSize": "var(--wp--custom--layout--wide-size)"
		},
		"shadow": {
			"defaultPresets": false,
			"presets": null
		},
		"spacing": {
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"units": ["px", "em", "rem"],
			"spacingSizes": null
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textAlign": false,
			"textDecoration": false,
			"textTransform": false,
			"writingMode": false,
			"fontSizes": null
		},
		"custom": {
			"color": {
				"brand": "oklch(0.4 0.15 25)",
				"green": "oklch(0.4 0.11 145)",
				"ocean": "oklch(0.4 0.18 262)",
				"focus": "oklch(0.9 0.17 99)",
				"cream": "oklch(0.98 0 0)",
				"guide": "oklch(0.95 0 0)",
				"border": "oklch(0.8 0 0)",
				"smoke": "oklch(0.46 0.046 265)",
				"text": "oklch(0.23 0.023 265)",
				"black": "oklch(0.12 0.003 87)",
				"button": "var(--wp--custom--color--green)",
				"link": "var(--wp--custom--color--ocean)"
			},
			"blockSpace": {
				"lg": "clamp(3rem, 1.3333rem + 5.5556vw, 4rem)",
				"rg": "clamp(1rem, 0.7917rem + 0.6944vw, 1.125rem)"
			},
			"fontSize": {
				"xl": "clamp(1.5rem, 0.6667rem + 2.7778vw, 2rem)",
				"lg": "clamp(1.25rem, 0.8333rem + 1.3889vw, 1.5rem)",
				"rg": "clamp(1rem, 0.7917rem + 0.6944vw, 1.125rem)",
				"sm": "clamp(0.875rem, 0.7708rem + 0.3472vw, 0.9375rem)",
				"xm": "clamp(0.75rem, 0.6458rem + 0.3472vw, 0.8125rem)"
			},
			"layout": {
				"contentSize": "736px",
				"sidebarSize": "336px",
				"wideSize": "1200px"
			},
			"lineHeight": {
				"lg": "calc(1ex / 0.32)",
				"rg": "1.5",
				"sm": "1.2"
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--custom--color--cream)",
			"text": "var(--wp--custom--color--text)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--custom--color--button)",
					"text": "var(--wp--custom--color--cream)"
				},
				"spacing": {
					"padding": {
						"top": "10px",
						"bottom": "10px",
						"left": "20px",
						"right": "20px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--custom--font-size--sm)",
					"lineHeight": "var(--wp--custom--line-height--sm)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				}
			}
		},
		"blocks": {
			"core/pullquote": {
				"typography": {
					"textAlign": "left",
					"fontSize": "inherit",
					"lineHeight": "inherit"
				}
			}
		}
	}
}
