{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "color": {
      "background": false,
      "button": false,
      "caption": false,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "heading": false,
      "link": false,
      "text": false,
      "duotone": null,
      "gradients": null,
      "palette": null
    },
    "dimensions": {
      "defaultAspectRatios": false,
      "aspectRatios": null
    },
    "shadow": {
      "defaultPresets": false,
      "presets": null
    },
    "spacing": {
      "customSpacingSize": false,
      "defaultSpacingSizes": false,
      "units": ["rem", "vh", "vw", "%"],
      "spacingScale": null
    },
    "typography": {
      "customFontSize": false,
      "defaultFontSizes": false,
      "dropCap": false,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textAlign": false,
      "textDecoration": false,
      "textTransform": false,
      "writingMode": false,
      "fontSizes": null
    },
    "layout": {
      "contentSize": "736px",
      "wideSize": "1200px"
    },
    "custom": {
      "blue": "oklch(0.25 0.15 240)",
      "link": "var(--wp--custom--blue)",
      "brand": "oklch(0.25 0.15 0)",
      "black": "oklch(0.12 0.03 87)",
      "smoke": "oklch(0.4 0.01 0)",
      "focus": "oklch(0.8 0.17 80)",
      "white": "oklch(0.98 0 0)",
      "guide": "oklch(0.93 0.01 0)",
      "border": "oklch(0.8 0.01 0)",
      "forest": "oklch(0.4 0.125 140)",
      "button": "var(--wp--custom--forest)"
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--custom--white)",
      "text": "var(--wp--custom--black)"
    },
    "elements": {
      "button": {
        "border": {
          "radius": ".5rem"
        },
        "color": {
          "background": "var(--wp--custom--button)",
          "text": "var(--wp--custom--white)"
        },
        "spacing": {
          "padding": {
            "top": "12px",
            "right": "20px",
            "bottom": "12px",
            "left": "20px"
          }
        },
        "typography": {
          "fontSize": "1rem",
          "lineHeight": "1.2"
        }
      }
    },
    "blocks": {
      "core/pullquote": {
        "typography": {
          "fontSize": "inherit",
          "lineHeight": "1.5"
        }
      }
    }
  }
}
