{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "color": {
      "background": false,
      "button": false,
      "caption": false,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "heading": false,
      "link": false,
      "text": false,
      "duotone": null,
      "gradients": null,
      "palette": null
    },
    "dimensions": {
      "defaultAspectRatios": false,
      "aspectRatios": null
    },
    "shadow": {
      "defaultPresets": false,
      "presets": null
    },
    "spacing": {
      "customSpacingSize": false,
      "defaultSpacingSizes": false,
      "units": ["rem", "vh", "vw", "%"],
      "spacingScale": null
    },
    "typography": {
      "customFontSize": false,
      "defaultFontSizes": false,
      "dropCap": false,
      "fontSizes": null,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textAlign": false,
      "textDecoration": false,
      "textTransform": false,
      "writingMode": false
    },
    "layout": {
      "contentSize": "750px",
      "wideSize": "1200px"
    },
    "custom": {
      "black": "rgb(17 23 29)",
      "gray": "rgb(75 85 95)",
      "red": "rgb(165, 28, 48)",
      "blue": "rgb(0 51 166)",
      "green": "rgb(0 112 60)",
      "yellow": "rgb(250 191 70)",
      "white": "rgb(251 251 251)",
      "background": "rgb(246 248 250)",
      "border": "rgb(177 180 183)",
      "marker": "rgb(177 180 183 / 17%)",
      "line-height-tight": 1.2,
      "line-height-normal": 1.5,
      "line-height-loose": 1.7,
      "font-body": "\"Agisty Body\", system-ui, -apple-system, sans-serif",
      "font-head": "\"Agisty Head\", system-ui, -apple-system, sans-serif",
      "font-mono": "Menlo, \"Fira Mono\", Consolas, Monaco, monospace"
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--custom--white)",
      "text": "var(--wp--custom--black)"
    },
    "typography": {
      "fontFamily": "var(--wp--custom--font-body)",
      "fontSize": "var(--wp--preset--font-size--normal)",
      "lineHeight": "var(--wp--custom--line-height-loose)",
      "fontWeight": "400"
    }
  }
}
