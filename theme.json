{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "color": {
      "background": false,
      "button": false,
      "caption": false,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "heading": false,
      "link": false,
      "text": false,
      "duotone": null,
      "gradients": null,
      "palette": null
    },
    "dimensions": {
      "defaultAspectRatios": false,
      "aspectRatios": null
    },
    "shadow": {
      "defaultPresets": false,
      "presets": null
    },
    "spacing": {
      "customSpacingSize": false,
      "defaultSpacingSizes": false,
      "units": ["rem", "vh", "vw", "%"],
      "spacingScale": null
    },
    "typography": {
      "customFontSize": false,
      "defaultFontSizes": false,
      "dropCap": false,
      "fontSizes": null,
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": false,
      "textAlign": false,
      "textDecoration": false,
      "textTransform": false,
      "writingMode": false
    },
    "layout": {
      "contentSize": "750px",
      "wideSize": "1200px"
    },
    "custom": {
      "blue": "rgb(0 50 150)",
      "bttn": "rgb(130 30 30)",
      "gray": "rgb(70 80 90)",
      "black": "rgb(17 23 29)",
      "focus": "rgb(250 190 70)",
      "white": "rgb(250 250 250)",
      "border": "rgb(from var(--wp--custom--gray) r g b / 29%)",
      "guides": "rgb(from var(--wp--custom--gray) r g b / 11%)",
      "line-height-2": "1.2",
      "line-height-5": "1.5",
      "line-height-c": "calc(1ex / 0.32)",
      "font-body": "\"Agisty Body\", system-ui, -apple-system, sans-serif",
      "font-head": "\"Agisty Head\", system-ui, -apple-system, sans-serif",
      "font-mono": "\"Agisty Mono\", \"SF Mono\", \"Fira Mono\", Menlo, monospace"
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--custom--white)",
      "text": "var(--wp--custom--black)"
    },
    "typography": {
      "fontFamily": "var(--wp--custom--font-body)",
      "fontSize": "var(--wp--preset--font-size--normal)",
      "lineHeight": "var(--wp--custom--line-height-5)",
      "fontWeight": "400"
    },
    "elements": {
      "button": {
        "border": {
          "radius": ".5rem"
        },
        "color": {
          "background": "var(--wp--custom--bttn)",
          "text": "var(--wp--custom--white)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--normal)",
          "lineHeight": "var(--wp--custom--line-height-2)"
        }
      }
    }
  }
}
